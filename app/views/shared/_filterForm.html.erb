<%= search_form_for @q do |f| %>

<div id = "outer">
<div class = "innerS">
  <div class = "BoxLabel"><%= f.label "Tags" %></div>
  <%= f.search_field :AllTags_cont || :AllTags_matches %>
  </div>
<div class = "innerS">
  <div class = "BoxLabel"><%= f.label "Title" %></div>
  <%= f.search_field :title_cont || :title_matches %>
  </div>
<div class = "innerS">
  <div class = "BoxLabel"><%= f.label "Username" %></div>
  <%= f.search_field :user_username_matches %>
  </div>
  <br>
 <div class = "innerS">
  <div class = "BoxLabel"><%= f.label "NSFW" %></div>
  <%= f.check_box :SFW_false%>
  </div>  
  <%= f.submit %>
</div>
<% end %>